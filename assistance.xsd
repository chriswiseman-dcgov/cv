																					<xs:sequence>
																						<xs:element name="type"/>
																						<xs:element name="amount"/>
																						<xs:element name="frequency"/>
																						<xs:element name="start_date"/>
																						<xs:element name="end_date"/>
																					</xs:sequence>
																				</xs:complexType>
																			</xs:element>
																		</xs:sequence>
																	</xs:complexType>
																</xs:element>
															</xs:sequence>
														</xs:complexType>
													</xs:element>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
