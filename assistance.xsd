																		<xs:sequence>
																			<xs:element name="benefit">
																				<xs:complexType>
																					<xs:sequence>
																						<xs:element name="benefit_type"/>
																						<xs:element name="benefit_start_date"/>
																						<xs:element name="benefit_end_date"/>
																					</xs:sequence>
																				</xs:complexType>
																			</xs:element>
																		</xs:sequence>
																	</xs:complexType>
																</xs:element>
