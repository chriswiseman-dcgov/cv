															<xs:sequence>
																<xs:element name="relationship" maxOccurs="unbounded">
																	<xs:complexType>
																		<xs:sequence>
																			<xs:element name="relationship_type"/>
																			<xs:element name="related_participant_id"/>
																		</xs:sequence>
																	</xs:complexType>
																</xs:element>
															</xs:sequence>
														</xs:complexType>
													</xs:element>
													<xs:element name="aptc_amount"/>
													<xs:element name="csr"/>
