<?xml version="1.0" encoding="UTF-8"?>
<!-- edited with XMLSpy v2014 (http://www.altova.com) by Dan Thomas (DC Department of Human Services DCAS Project) -->
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://openhbx.org/api/terms/1.0" targetNamespace="http://openhbx.org/api/terms/1.0" elementFormDefault="qualified">
	<xs:include schemaLocation="common.xsd"/>
	<xs:element name="application_group" type="ApplicationGroupType"/>
	<xs:element name="enrollment_status" type="EnrollmentStatusType"/>
	<xs:element name="household" type="HouseholdType"/>
	<xs:element name="eligibility_determination" type="EligibilityDeterminationType"/>
	<xs:element name="individual" type="IndividualType"/>
	<xs:element name="individual_event" type="IndividualEventType"/>
	<xs:element name="member" type="MemberRoleType"/>
	<xs:element name="individual_uri" type="xs:anyURI" substitutionGroup="resource_instance_uri">
		<xs:annotation>
			<xs:documentation>Dererferenceable identifier for individual instance, e.g. urn:dc0:individual:_id</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="update" type="UpdateType"/>
	<xs:complexType name="IndividualType">
		<xs:sequence>
			<xs:element name="authority_hbx_member_uri" type="xs:anyURI"/>
			<xs:element name="authority_hbx_member_id" type="xs:string"/>
			<xs:element name="person" type="PersonType"/>
			<xs:element name="member_roles" type="MemberRoleType"/>
			<xs:element name="employee_roles" type="EmployeeRoleType" maxOccurs="unbounded"/>
			<xs:element name="responsible_party_roles" type="ResponsiblePartyRoleType"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="IndividualEventType">
		<xs:annotation>
			<xs:documentation>Message format for Individual resource event</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:restriction base="EventType">
				<xs:sequence>
					<xs:element name="hbx_id" type="xs:string"/>
					<xs:element name="event_name" type="IndividualEventNameType"/>
					<xs:element name="qualifying_reason" type="SpecialEnrollmentPeriodType"/>
					<xs:element ref="individual_uri"/>
					<xs:element name="sender_uri" type="xs:anyURI"/>
					<xs:element name="submitted_timestamp" type="xs:dateTime"/>
				</xs:sequence>
			</xs:restriction>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="PreviousMemberType">
		<xs:sequence>
			<xs:group ref="HbxIdentifierGroup"/>
			<xs:element name="gender" type="GenderType"/>
			<xs:element name="dob" type="SimpleDateType"/>
			<xs:element name="ssn" type="FederalTaxIdType" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="PreviousIndividualType">
		<xs:sequence>
			<xs:element name="name_prefix" type="xs:string" minOccurs="0"/>
			<xs:element name="name_first" type="xs:string"/>
			<xs:element name="name_middle" type="xs:string" minOccurs="0"/>
			<xs:element name="name_last" type="xs:string"/>
			<xs:element name="name_suffix" type="xs:string" minOccurs="0"/>
			<xs:element name="name_full" type="xs:string" minOccurs="0"/>
			<xs:element name="member_change" type="PreviousMemberType" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="UpdateType">
		<xs:sequence>
			<xs:element name="change_type" type="UpdateKindType" minOccurs="1" maxOccurs="unbounded"/>
			<xs:element ref="individual"/>
			<xs:element name="previous_information" type="PreviousIndividualType"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="MemberRoleType">
		<xs:sequence>
			<xs:element name="gender" type="GenderType"/>
			<xs:element name="dob" type="SimpleDateType">
				<xs:annotation>
					<xs:documentation>Date of Birth</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ssn" type="FederalTaxIdType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Social Security Number</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="tobacco_user" type="TobaccoUseType" default="unknown"/>
			<xs:element name="language" type="LanguageType" minOccurs="0"/>
			<xs:group ref="HbxIdentifierGroup"/>
			<xs:element name="carrier_assigned_member_id" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Carrier-assigned identifier</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="person" type="PersonType"/>
			<xs:element name="relationship" type="RelationshipType"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="EmployeeRoleType">
		<xs:sequence>
			<xs:element name="hbx_employer_id" type="xs:string"/>
			<xs:element name="hbx_employer_uri" type="xs:anyURI"/>
			<xs:element name="employment_status" type="EmploymentStatusType" minOccurs="0"/>
			<xs:element name="eligibility_start_date" type="SimpleDateType" minOccurs="0"/>
			<xs:element name="eligibility_end_date" type="SimpleDateType" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ResponsiblePartyRoleType">
		<xs:sequence>
			<xs:element name="entity_identifier" type="EntityType"/>
			<xs:element name="hbx_organization_uri" type="xs:anyURI" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="EnrolleeType">
		<xs:sequence>
			<xs:element name="premium_amount" type="CurrencyType"/>
			<xs:element name="disabled" type="xs:boolean"/>
			<xs:element name="benefit_status" type="BenefitStatusType" minOccurs="0"/>
			<xs:element name="employment_status" type="EmploymentStatusType" default="urn:dc0:terms:employment_status#active"/>
			<xs:element name="relationship" type="RelationshipType"/>
			<xs:element name="carrier_assigned_member_id" type="xs:string"/>
			<xs:element name="carrier_assigned_policy_id" type="xs:string"/>
			<xs:element name="coverage_start_date" type="SimpleDateType"/>
			<xs:element name="coverage_end_date" type="CurrencyType"/>
			<xs:element name="coverage_status"/>
			<xs:element name="member" type="MemberRoleType"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ApplicationGroupType">
		<xs:sequence>
			<xs:element ref="individual" minOccurs="1" maxOccurs="unbounded"/>
			<xs:element ref="household" minOccurs="1" maxOccurs="unbounded"/>
			<xs:element ref="enrollment_status" minOccurs="1"/>
			<xs:element name="applicant_relationships" type="PersonRelationshipType" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Relationships between primary applicant and each of the other appliicants.  Will include head of household self-pointer at minimum</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="HouseholdType">
		<xs:annotation>
			<xs:documentation>Applicant sub-grouping for purpose of financial eligibility determination and enrollment</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element ref="eligibility_determination" maxOccurs="unbounded"/>
			<xs:element name="household_relationships" type="RelationshipType" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="EligibilityDeterminationType">
		<xs:sequence>
			<xs:element name="authority_source_uri" minOccurs="0">
				<xs:annotation>
					<xs:documentation>URN reference to service or agency responsible for determination results</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="event_id" minOccurs="0">
				<xs:annotation>
					<xs:documentation>reference number for determination</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="date" type="SimpleDateType">
				<xs:annotation>
					<xs:documentation>Date of determination</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="maximum_aptc" type="CurrencyType">
				<xs:annotation>
					<xs:documentation>Total tax credit amount</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="magi_amount" type="CurrencyType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Modified Adjusted Gross Income figure used to determine elibility</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="PersonRelationshipType">
		<xs:sequence>
			<xs:element name="subject_person_uri" type="xs:anyURI"/>
			<xs:element name="relationship_kind" type="RelationshipType">
				<xs:annotation>
					<xs:documentation>change to substitution group</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="object_person_uri" type="xs:anyURI"/>
		</xs:sequence>
	</xs:complexType>
	<xs:simpleType name="IndividualEventNameType">
		<xs:restriction base="xs:anyURI">
			<xs:enumeration value="urn:dc0:events:individual#added"/>
			<xs:enumeration value="urn:dc0:events:individual#updated"/>
			<xs:enumeration value="urn:dc0:events:individual#deleted"/>
			<xs:enumeration value="urn:dc0:events:individual#qhp_withdrawn"/>
			<xs:enumeration value="urn:dc0:events:individual#qhp_selected"/>
			<xs:enumeration value="urn:dc0:events:individual#sep_started"/>
			<xs:enumeration value="urn:dc0:events:individual#sep_ended"/>
			<xs:enumeration value="urn:dc0:events:individual#effectuated"/>
			<xs:enumeration value="urn:dc0:events:individual#authority_member_set"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="UpdateKindType">
		<xs:restriction base="xs:string">
			<xs:enumeration value="name_change"/>
			<xs:enumeration value="address_change"/>
			<xs:enumeration value="email_change"/>
			<xs:enumeration value="phone_change"/>
			<xs:enumeration value="employment_change"/>
			<xs:enumeration value="member_change"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="TobaccoUseType">
		<xs:restriction base="xs:string">
			<xs:enumeration value="true"/>
			<xs:enumeration value="false"/>
			<xs:enumeration value="unknown"/>
		</xs:restriction>
	</xs:simpleType>
</xs:schema>
