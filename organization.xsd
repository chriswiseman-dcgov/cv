		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="BenefitGroupType">
		<xs:annotation>
			<xs:documentation>A named set of benefits applicable to a particular group.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="name" type="xs:string"/>
			<xs:element name="reference_plan" type="PlanLinkType" minOccurs="0" maxOccurs="1"/>
			<xs:element name="elected_plans">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="elected_plan" type="PlanLinkType" minOccurs="0" maxOccurs="unbounded"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:choice>
				<xs:element name="federal_contributions" type="xs:anyType"/>
				<xs:element name="relationship_benefits" type="RelationshipBenefitsListType"/>
			</xs:choice>
		</xs:sequence>
	</xs:complexType>
