							<xs:documentation>URN(s) for the employer instance that is subject of this event</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element ref="extended_resource_instance_uri" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>URN(s) for additional uris which may be used to reference the subject of this event.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="body" type="OrganizationServiceEventBodyType"/>
				</xs:sequence>
			</xs:restriction>
		</xs:complexContent>
	</xs:complexType>
	<xs:simpleType name="EmployerDigestEventNameType">
		<xs:restriction base="xs:anyURI">
			<xs:enumeration value="urn:openhbx:events:v1:employer#digest_period_ended"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="EmployerDigestServiceEventBodyType">
		<xs:complexContent>
			<xs:restriction base="ServiceBodyType">
				<xs:sequence>
					<xs:element name="employer_events">
						<xs:annotation>
							<xs:documentation>Collection of employer profile event messages</xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:sequence>
								<xs:element name="coverage_period" type="DateTimeRangeType">
									<xs:annotation>
										<xs:documentation>The start and end timestamps for events listed in this digest instance</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="employer_event" type="EmployerEventType" minOccurs="0" maxOccurs="unbounded"/>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
				</xs:sequence>
			</xs:restriction>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="EmployerDigestEventType">
		<xs:annotation>
			<xs:documentation>A full digest of employer changes, submitted to Carrier partners as a batch.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:restriction base="ServiceEventType">
				<xs:sequence>
					<xs:element name="event_name" type="EmployerDigestEventNameType"/>
					<xs:element ref="resource_instance_uri">
						<xs:annotation>
							<xs:documentation>A URI uniquely referencing the carrier receiving the notification.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="body" type="EmployerDigestServiceEventBodyType"/>
				</xs:sequence>
			</xs:restriction>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="employer_digest_event" type="EmployerDigestEventType">
		<xs:annotation>
			<xs:documentation>A compilation of employer events</xs:documentation>
		</xs:annotation>
	</xs:element>
</xs:schema>
