	<xs:complexType name="PlanYearType">
		<xs:annotation>
			<xs:documentation>A period of benefits availble to an entity.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="plan_year_start" type="SimpleDateType">
				<xs:annotation>
					<xs:documentation>Date when employer-sponsored benefit eligibility begins</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="plan_year_end" type="SimpleDateType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Date when employer-sponsored benefit eligibility ends</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="fte_count" type="xs:integer">
				<xs:annotation>
					<xs:documentation>Number of full-time-employees</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="pte_count" type="xs:integer" default="0">
				<xs:annotation>
					<xs:documentation>Number of part-time employees</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="open_enrollment_start" type="SimpleDateType">
				<xs:annotation>
					<xs:documentation>Open enrollment period start date</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="open_enrollment_end" type="SimpleDateType">
				<xs:annotation>
					<xs:documentation>Open enrollment period end date</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="benefit_groups" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="benefit_group" type="BenefitGroupType" maxOccurs="unbounded"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:group ref="ResourceTimestampGroup"/>
		</xs:sequence>
	</xs:complexType>
