	<xs:simpleType name="EmployerEventNameType">
		<xs:restriction base="xs:anyURI">
			<xs:enumeration value="urn:openhbx:events:v1:employer#created">
				<xs:annotation>
					<xs:documentation>A new employer has successfully registered on the exchange.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="urn:openhbx:events:v1:employer#address_changed">
				<xs:annotation>
					<xs:documentation>An employer's office address has changed</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="urn:openhbx:events:v1:employer#contact_changed">
				<xs:annotation>
					<xs:documentation>An employer has changed designated point-of-contact (POC) or that contact's information.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="urn:openhbx:events:v1:employer#fein_corrected">
				<xs:annotation>
					<xs:documentation>Correcting an employer's previously provided, erroneous Federal Employer Identification Number (FEIN) </xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="urn:openhbx:events:v1:employer#name_changed">
				<xs:annotation>
					<xs:documentation>The legal name or DBA of an employer  has changed.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="urn:openhbx:events:v1:employer#broker_added">
				<xs:annotation>
					<xs:documentation>An employer has designated a new broker representative</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="urn:openhbx:events:v1:employer#broker_terminated">
				<xs:annotation>
					<xs:documentation>A benefit sponsor has terminated an existing broker representative.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="urn:openhbx:events:v1:employer#general_agent_added">
				<xs:annotation>
					<xs:documentation>A broker has designated a GA for the referenced sponsor</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="urn:openhbx:events:v1:employer#general_agent_terminated">
				<xs:annotation>
					<xs:documentation>A broker has terminated an existing GA for referenced sponsor</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="urn:openhbx:events:v1:employer#benefit_coverage_initial_open_enrollment_ended">
				<xs:annotation>
					<xs:documentation>A new employer's first open enrollment period on the HBX has ended</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="urn:openhbx:events:v1:employer#benefit_coverage_initial_binder_paid">
				<xs:annotation>
					<xs:documentation>A new employer has submitted payment-in-full for first premium.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="urn:openhbx:events:v1:employer#benefit_coverage_initial_application_approved">
				<xs:annotation>
					<xs:documentation>A new employer has met requirements to enroll employees through the HBX.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="urn:openhbx:events:v1:employer#benefit_coverage_initial_application_eligible">
				<xs:annotation>
					<xs:documentation>A new employer has met both eligibility and enrollment requirements to offer benefit coverage through the HBX.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="urn:openhbx:events:v1:employer#benefit_coverage_period_terminated_voluntary">
				<xs:annotation>
					<xs:documentation>An existing sponsor has elected to discontinue offering benefits through the HBX.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="urn:openhbx:events:v1:employer#benefit_coverage_period_terminated_nonpayment">
				<xs:annotation>
					<xs:documentation>An existing employer's sponsorship, and associated employee enrollments, are discontinued due to premium non-payment.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="urn:openhbx:events:v1:employer#benefit_coverage_period_terminated_relocated">
				<xs:annotation>
					<xs:documentation>An existing sponsor is ineligible to offer benefits through the HBX due to relocating primary office outside service area.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="urn:openhbx:events:v1:employer#benefit_coverage_renewal_open_enrollment_ended">
				<xs:annotation>
					<xs:documentation>A returning sponsor's open enrollment period for has ended</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="urn:openhbx:events:v1:employer#benefit_coverage_renewal_carrier_dropped">
				<xs:annotation>
					<xs:documentation>A returning employer has discontinued offering an incumbent carrier's benefits for the renewal coverage period</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="urn:openhbx:events:v1:employer#benefit_coverage_renewal_terminated_voluntary">
				<xs:annotation>
					<xs:documentation>A returning employer has elected to discontinue offering benefits through the HBX</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="urn:openhbx:events:v1:employer#benefit_coverage_renewal_terminated_ineligible">
				<xs:annotation>
					<xs:documentation>A returning employer has failed to meet eligibility and/or enrollment requirements to offer benefits for the renewal coverage period</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="urn:openhbx:events:v1:employer#benefit_coverage_renewal_application_eligible">
				<xs:annotation>
					<xs:documentation>A returning employer has met both eligibility and enrollment requirements to offer benefits for the renewal coverage period</xs:documentation>
				</xs:annotation>
			</xs:enumeration>			
			<xs:enumeration value="urn:openhbx:events:v1:employer#benefit_coverage_period_expired">
				<xs:annotation>
					<xs:documentation>A new or returning employer is ineligible to offer benefits through the HBX due to not meeting eligibility and/or enrollment requirements in a timely manner</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="urn:openhbx:events:v1:employer#benefit_coverage_period_reinstated">
				<xs:annotation>
					<xs:documentation>A new or returning employer is being re-activated after previously being terminated from the exchange in the middle of their coverage year.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="OrganizationServiceEventBodyType">
		<xs:complexContent>
			<xs:restriction base="ServiceBodyType">
				<xs:sequence>
					<xs:element name="organization" type="OrganizationWithRolesType"/>
				</xs:sequence>
			</xs:restriction>
		</xs:complexContent>
	</xs:complexType>
